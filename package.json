{
	"name": "@tknf/hono-utils",
	"version": "0.5.0",
	"description": "Utility functions and middlewares for Hono.",
	"keywords": [
		"typescript",
		"javascript",
		"hono",
		"server",
		"app",
		"utility",
		"session"
	],
	"repository": {
		"url": "https://github.com/tknf/hono-utils"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"license": "MIT",
	"author": "tknf <dev@tknf.net>",
	"type": "module",
	"exports": {
		"./csrf": {
			"types": "./dist/csrf/index.d.ts",
			"import": "./dist/csrf/index.js",
			"require": "./dist/csrf/index.cjs"
		},
		"./session": {
			"types": "./dist/session/index.d.ts",
			"import": "./dist/session/index.js",
			"require": "./dist/session/index.cjs"
		},
		"./validator": {
			"types": "./dist/validator/index.d.ts",
			"import": "./dist/validator/index.js",
			"require": "./dist/validator/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"lint": "biome lint",
		"format": "biome format",
		"check": "biome check",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "npm run build && npm run typecheck && npm run check",
		"examples": "tsx watch examples/index.ts"
	},
	"peerDependencies": {
		"hono": ">=4.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.6",
		"@hono/node-server": "^1.19.5",
		"@standard-schema/spec": "^1.0.0",
		"@vitest/coverage-v8": "^3.2.4",
		"arktype": "^2.1.20",
		"hono": "^4.10.1",
		"tsdown": "^0.15.8",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"valibot": "^1.1.0",
		"vitest": "^3.2.4",
		"zod": "^3.24.0"
	}
}
